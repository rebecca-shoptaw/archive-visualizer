{
  "name": "archive-visualizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://rebecca-shoptaw.github.io/archive-visualizer",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
